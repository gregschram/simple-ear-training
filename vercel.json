{
  "git": {
    "deploymentEnabled": false
  },
  "version": 2,
  "builds": [
    {
      "src": "generate-audio-list.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["audio/**/*"]
      }
    },
    {
      "src": "audio/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/audio/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "Accept-Ranges": "bytes"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
