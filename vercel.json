{
  "git": {
    "deploymentEnabled": false
  },
 "version": 2,
  "builds": [
    {
      "src": "generate-audio-list.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/audio/(.*)",
      "dest": "/audio/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
