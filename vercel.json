{
  "git": {
    "deploymentEnabled": false
  },
  "version": 2,
  "builds": [
    {
      "src": "audio/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/audio/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/audio/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
